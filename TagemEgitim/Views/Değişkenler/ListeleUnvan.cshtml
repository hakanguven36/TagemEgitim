@model IEnumerable<TagemEgitim.Models.Ünvan>

<table style="width:20em">
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.isim)
            </td>
            <td>
                <btn class="btn btn-outline-warning" onclick="UnvanDuzenleFormAc(@item.ID)"><span class="fas fa-pen"></span></btn>
                <btn class="btn btn-outline-danger" onclick="UnvanSil(@item.ID)"><span class="fas fa-trash"></span></btn>
            </td>
        </tr>
    }
</table>
